exported interface IShip imm { }
abstract func getFuel(virtual ship *IShip) int;

exported struct Firefly imm { fuel int; }
impl IShip for Firefly;
func getFuel(ship *Firefly impl IShip) int { ship.fuel }

exported func valeMakeShip() IShip {
  Firefly(42)
}

extern func cMakeShip() IShip;

exported func main() int {
  res = cMakeShip();
  ret getFuel(*res);
}
