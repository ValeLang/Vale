export [#3]<imm>int as ImmIntArray;

fn valeSumFuel(arr [#3]<imm>int) int export {
  total = 0;
  i = 0;
  while (i < arr.len()) {
    set total = total + arr[i];
    set i = i + 1;
  }
  ret total;
}

fn cSumFuel(arr [#3]<imm>int) int extern;

fn main() int export {
  a = #[#3]((i) => { 13 + i });
  ret cSumFuel(a);
}
