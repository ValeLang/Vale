
func has<E, F>(arr &[]<_>E, elem E, equator F) bool {
  i = 0;
  while (i < arr.len()) {
    if (equator(arr[i], elem)) {
      return true;
    }
    set i = i + 1;
  }
  return false;
}

func has<E>(arr &[]<_>E, elem E) bool {
  has(arr, elem, ==)
}

func has<E, F>(seq &[#_]<_>E, elem E, equator F) bool {
  i = 0;
  while (i < seq.len()) {
    if (equator(seq[i], elem)) {
      return true;
    }
    set i = i + 1;
  }
  return false;
}

func has<E>(seq &[#_]<_>E, elem E) bool {
  return has(seq, elem, ==);
}
