
fn str(b bool) str {
  = if (b) { "true" } else { "false" }
}

fn +(i int, s str) str {
  str(i) + s
}

fn +(s str, i int) str {
  s + str(i)
}

fn +(s str, b bool) str {
  s + str(b)
}

fn +(b bool, s str) str {
  str(b) + s
}

fn +(f float, s str) str {
  str(f) + s
}

fn +(s str, f float) str {
  s + str(f)
}
