
fn str(b bool) str {
  ret if (b) { "true" } else { "false" }
}

fn +(i int, s str) str {
  ret str(i) + s;
}

fn +(s str, i int) str {
  ret s + str(i);
}

fn +(s str, b bool) str {
  ret s + str(b);
}

fn +(b bool, s str) str {
  ret str(b) + s;
}

fn +(f float, s str) str {
  ret str(f) + s;
}

fn +(s str, f float) str {
  ret s + str(f);
}
