
func void() { }

func str(b bool) str {
  if (b) { "true" } else { "false" }
}

func +(i int, s str) str {
  str(i) + s
}

func +(s str, i int) str {
  s + str(i)
}

func +(s str, i i64) str {
  s + str(i)
}
func +(i i64, s str) str {
  str(i) + s
}

func +(s str, b bool) str {
  s + str(b)
}

func +(b bool, s str) str {
  str(b) + s
}

func +(f float, s str) str {
  str(f) + s
}

func +(s str, f float) str {
  s + str(f)
}
